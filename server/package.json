{
  "name": "fabo_backend",
  "version": "0.0.2",
  "private": true,
  "description": "fabo_backend API server",
  "main": "handler.js",
  "type": "module",
  "scripts": {
    "prestart": "mongo admin --eval 'db.shutdownServer()'; mongod --config ./db/mongodb.conf",
    "start": "cross-env NODE_ENV=development node handler.js --debug",
    "dev": "sls offline --stage dev --printOutput start",
    "dev_cache": "sls offline --stage dev --allowCache --printOutput start",
    "dev_prod": "sls offline --stage prod --printOutput start",
    "deploy": "sls deploy --stage prod --verbose",
    "poststart": "mongo admin --eval 'db.shutdownServer()'"
  },
  "keywords": [],
  "author": "Francisco Aboim",
  "license": "CC",
  "dependencies": {
    "@flybondi/serverless-rollup-plugin": "^1.4.0",
    "@vendia/serverless-express": "^4.5.3",
    "api-query-params": "^5.3.1",
    "aws-sdk": "^2.1073.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.6.0",
    "mongoose": "^5.13.14",
    "mongoose-validator": "github:fpaboim/mongoose-validator",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rollup": "^2.67.2",
    "serverless-api-gateway-throttling": "^1.2.2",
    "serverless-externals-plugin": "^0.2.6",
    "serverless-offline": "^8.4.0",
    "slugify": "^1.6.5",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "rollup-plugin-node-externals": "^4.0.0",
    "serverless-dotenv-plugin": "^3.12.2"
  }
}
